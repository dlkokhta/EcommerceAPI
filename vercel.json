{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  